
@{
    ViewBag.Title = "UploadDictionaryLists";
}

<h2>UploadDictionaryLists</h2>
@using (Html.BeginForm("UploadDictionaryLists", "StickerLists", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <div class="form-group">
        <input type="file" id="dataFile" name="file" />
    </div>

    <div class="form-group">
        <input type="submit" value="Cargar" class="btn btn-default" />
    </div>

}
